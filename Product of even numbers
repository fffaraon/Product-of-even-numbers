#include <iostream>
#include<math.h>
int main() 
{
    const int n = 6; 
    int mas[n];
    for (int i = 0;i < n;i++)
        mas[i] = rand() % 100; 
    int p = 1;

    for (int i = 0;i < n;i++)
        if (i % 2 == 0)
            p *= mas[i];

    for (int i = 0;i < n;i++)
    {
        printf("%d\n", mas[i]);
    }
    return 0;
}
